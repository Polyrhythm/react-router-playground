  },
  "homepage": "https://github.com/knowbody/react-router-playground#readme",
  "devDependencies": {
    "babel-core": "^5.4.7",
    "babel-eslint": "^3.1.9",
    "babel-jest": "^5.3.0",
    "babel-loader": "^5.1.2",
    "babel-plugin-react-transform": "^1.0.1",
    "eslint": "^1.3.1",
    "eslint-plugin-react": "^2.3.0",
    "express": "^4.13.3",
    "jest-cli": "^0.5.8",
    "react-addons-test-utils": "^0.14.2",
    "react-transform-catch-errors": "^0.1.1",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.0.1",
